<div class="container">
  <div class="row">
    <div class="col-md-4">
    </div>
  <div class="col-md-4">
<h1> Create an Account </h1>

<%= simple_form_for @user do |f| %>
  <%= f.input :username %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
</div>
</div>
</div>
