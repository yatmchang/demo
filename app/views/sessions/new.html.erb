<div class="container">
  <div class="row">
    <div class="col-md-4">
    </div>
  <div class="col-md-4">
<h1> Sign In </h1>

<%= form_tag sessions_path do %>
  <div class="form-group">
      <%= label_tag :username %>
      <%= text_field_tag :username, "", class: "form-control"%>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control"%>
  </div>
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
<% end %>
</div>
</div>
</div>
